{
    "manifest_version": 3,
    "name": "Arc Keeper",
    "version": "1.0",
    "description": "Episode/Chapter progress tracker + Anime themed tab titles",
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
    }
}
